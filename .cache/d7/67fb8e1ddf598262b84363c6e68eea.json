 &__button {\r\n        display: none;\r\n\r\n        @media (min-width: $desktop_breakpoint) {\r\n                display: block;\r\n            }\r\n        }\r\n    }\r\n\r\n// Pagination\r\n\r\n.menu {\r\n    background-image: url(\"../images/icon-hamburger.svg\");\r\n    background-repeat: no-repeat;\r\n    background-size: 24px 24px;\r\n    background-position: 50% 50%;\r\n    color: transparent;\r\n\r\n    transition: all 0.3s;\r\n    box-sizing: border-box;\r\n    cursor: pointer;\r\n    z-index: 99;\r\n    height: 100%;\r\n    width: 100%;\r\n    height: 22px;\r\n    width: 22px;\r\n\r\n    @media (min-width: $desktop_breakpoint) {\r\n        display: none;\r\n    }\r\n}\r\n\r\nbody > label {\r\n    position: absolute;\r\n    color: transparent;\r\n    z-index: -1;\r\n    top: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    display: block;\r\n    opacity: 0;\r\n    transition: opacity 1s;\r\n    background-color: rgba(112, 112, 112, 0.25);\r\n  \r\n    @media (min-width: $desktop_breakpoint) {\r\n      display: none;\r\n    }\r\n}\r\n\r\ninput[type=\"checkbox\"]:checked ~ label {\r\n    z-index: 2;\r\n    opacity: 1;\r\n}\r\n\r\n.navigation {\r\n    text-align: center;\r\n    top: 10%;\r\n    left: 0;\r\n    position: fixed;\r\n    width: 85%;\r\n    margin-top: 60px;\r\n    transform: translateX(-110%);\r\n    transition: transform 250ms ease-in-out;\r\n    background-color: white;\r\n    padding-top: 30px;\r\n    padding-bottom: 30px;\r\n    padding-left: 20px;\r\n    border-radius: 5px;\r\n\r\n    &__list {\r\n        @include flex-layout();\r\n        flex-direction: column;\r\n        gap: 20px;\r\n        font-weight: 600;\r\n        color: $dark-blue;\r\n \r\n        @media (min-width: $desktop_breakpoint) {\r\n            @include flex-layout();\r\n            gap: 24px;\r\n        }\r\n    }\r\n\r\n\r\n    @media (min-width: $desktop_breakpoint) {\r\n        height: unset;\r\n        top: unset;\r\n        left: unset;\r\n        position: unset;\r\n        width: unset;\r\n        margin-top: 0;\r\n        transform: unset;\r\n        transition: unset;\r\n        background-color: unset;\r\n    }\r\n}\r\n\r\n// input[type=\"checkbox\"]:checked ~ * {\r\n//     background-color: $dark-grayish-blue;\r\n// }\r\n\r\ninput[type=\"checkbox\"]:checked ~ .container > .header > .navigation {\r\n    transform: translateX(10%);\r\n    z-index: 3;\r\n}\r\n\r\nbody > [type=\"checkbox\"]:checked ~ main {\r\n    z-index: 1;\r\n  } \r\n\r\n\r\ninput[type=\"checkbox\"]:checked ~ .container > .header > .menu {\r\n    transition: all 0.3s;\r\n    background-image: url(\"../images/icon-close.svg\");\r\n}\r\n\r\n\r\ninput[type=checkbox] {\r\n    transition: all 0.2s;\r\n    box-sizing: border-box;\r\n    display: none;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"dbe64da9a4285cdbf22c10f6c05a6464","cacheData":{"env":{}}}