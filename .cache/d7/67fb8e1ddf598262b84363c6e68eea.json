ext-decoration: none;\r\n\r\n    &:hover {\r\n        opacity: 0.8;\r\n    }\r\n}\r\n\r\nbutton {\r\n    cursor: pointer;\r\n    border: none;\r\n\r\n    &:hover {\r\n        opacity: 0.8;\r\n        border: none;\r\n    }\r\n\r\n    &:focus {\r\n        outline: none;\r\n        border: none;\r\n    }\r\n}\r\n\r\n.call-to-action {\r\n    background-color: $bright-red;\r\n    color: $vary-light-gray;\r\n    border-radius: 20px;\r\n    padding: 10px 22px;\r\n}","styles\\_variables.scss":"@mixin flex-layout {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n@mixin reset-list {\r\n    list-style: none;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n// Breakpoint\r\n\r\n$desktop_breakpoint: 1440px;\r\n\r\n\r\n// Color: \r\n\r\n$bright-red: hsl(12, 88%, 59%);\r\n$dark-blue: hsl(228, 39%, 23%);\r\n$dark-grayish-blue: hsl(227, 12%, 61%);\r\n$very-dark-blue: hsl(233, 12%, 13%);\r\n$very-pale-red: hsl(13, 100%, 96%);\r\n$vary-light-gray: hsl(0, 0%, 98%);","styles\\_container.scss":".container {\r\n    padding-left: 14px;\r\n    padding-right: 14px;\r\n\r\n    @media (min-width: $desktop_breakpoint) {\r\n        max-width: $desktop_breakpoint;\r\n        margin: auto;\r\n    }\r\n}","styles\\_header.scss":"@import \"./variables\";\r\n\r\n\r\n.header {\r\n    @include flex-layout();\r\n\r\n    &__button {\r\n        display: none;\r\n\r\n        @media (min-width: $desktop_breakpoint) {\r\n            display: block;\r\n        }\r\n    }\r\n}\r\n\r\n// Pagination\r\n\r\n.menu {\r\n    background-image: url(\"../images/icon-hamburger.svg\");\r\n    background-repeat: no-repeat;\r\n    background-size: 24px 24px;\r\n    background-position: 50% 50%;\r\n    color: transparent;\r\n\r\n    transition: all 0.3s;\r\n    box-sizing: border-box;\r\n    cursor: pointer;\r\n    z-index: 99;\r\n    height: 100%;\r\n    width: 100%;\r\n    height: 22px;\r\n    width: 22px;\r\n\r\n    @media (min-width: $desktop_breakpoint) {\r\n        display: none;\r\n    }\r\n}\r\n\r\n.navigation {\r\n    top: 10%;\r\n    left: 0;\r\n    position: fixed;\r\n    width: 80%;\r\n    margin-top: 60px;\r\n    transform: translateX(-110%);\r\n    transition: transform 250ms ease-in-out;\r\n    background-color: $vary-light-gray;\r\n    padding-top: 20px;\r\n    padding-bottom: 20px;\r\n    border-radius: 5px;\r\n\r\n    &__list {\r\n        @media (min-width: $desktop_breakpoint) {\r\n            @include flex-layout();\r\n            gap: 24px;\r\n        }\r\n    }\r\n\r\n\r\n    @media (min-width: $desktop_breakpoint) {\r\n        height: unset;\r\n        top: unset;\r\n        left: unset;\r\n        position: unset;\r\n        width: unset;\r\n        margin-top: 0;\r\n        transform: unset;\r\n        transition: unset;\r\n        background-color: unset;\r\n    }\r\n}\r\n\r\n// input[type=\"checkbox\"]:checked ~ * {\r\n//     background-color: $dark-grayish-blue;\r\n// }\r\n\r\ninput[type=\"checkbox\"]:checked ~ .navigation {\r\n    transform: translateX(10%);\r\n}\r\n\r\ninput[type=\"checkbox\"]:checked ~ .menu {\r\n    transition: all 0.3s;\r\n    background-image: url(\"../images/icon-close.svg\");\r\n}\r\n\r\n\r\ninput[type=checkbox] {\r\n    transition: all 0.3s;\r\n    box-sizing: border-box;\r\n    display: none;\r\n}\r\n\r\n.navigation {\r\n\r\n    \r\n}"},"lineCount":null}},"error":null,"hash":"adab552532f566c82533a5e8282c4f00","cacheData":{"env":{}}}